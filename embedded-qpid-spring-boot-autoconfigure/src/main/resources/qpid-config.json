{
  "name": "Embedded Broker",
  "modelVersion": "7.0",
  "authenticationproviders": [
    {
      "name": "plain",
      "type": "Plain",
      "users": [
        {
          "name": "${qpid.username}",
          "type": "managed",
          "password": "${qpid.password}"
        }
      ]
    }
  ],
  "ports": [
    {
      "name": "AMQP",
      "port": "${qpid.amqp_port}",
      "protocols": [
        "AMQP_0_9",
        "AMQP_0_9_1",
        "AMQP_1_0"
      ],
      "authenticationProvider": "plain",
      "virtualhostaliases": [
        {
          "name": "nameAlias",
          "type": "nameAlias"
        },
        {
          "name": "defaultAlias",
          "type": "defaultAlias"
        },
        {
          "name": "hostnameAlias",
          "type": "hostnameAlias"
        }
      ]
    }
  ],
  "virtualhostnodes": [
    {
      "name": "default",
      "type": "JSON",
      "defaultVirtualHostNode": "true",
      "virtualHostInitialConfiguration": "{\"type\" : \"Memory\",\"name\" : \"default\",\"modelVersion\" : \"6.0\"}"
    }
  ]
}